<rule>
  key $.userId
  pattern /code=[a-zA-Z0-9\._\-%]+/
  replace code=[REDACTED]
</rule>

<rule>
  key $.properties.userId
  pattern /access_token=[a-zA-Z0-9\._\-]+/
  replace access_token=[REDACTED]
</rule>